#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules/@antfu/eslint-config/bin/node_modules:/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules/@antfu/eslint-config/node_modules:/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules/@antfu/node_modules:/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules:/home/lay/chapter1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules/@antfu/eslint-config/bin/node_modules:/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules/@antfu/eslint-config/node_modules:/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules/@antfu/node_modules:/home/lay/chapter1/node_modules/.pnpm/@antfu+eslint-config@4.14.1_@vue+compiler-sfc@3.5.16_eslint@9.28.0_typescript@5.8.3/node_modules:/home/lay/chapter1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
